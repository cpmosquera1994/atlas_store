<nav aria-label="breadcrumb">
  <ol class="breadcrumb">
    <li class="breadcrumb-item"><%= link_to "Home", root_path %></li>
    <li class="breadcrumb-item active" aria-current="page">Banners</li>
  </ol>
</nav>
<h1>Banners</h1>
<div id="banners">
  <% @banners.each do |banner| %>
